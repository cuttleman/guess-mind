{
  "name": "guess-mind",
  "version": "1.0.0",
  "description": "Realtime Game with VanillaJS and SocketIO, Gulp, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/ddamjengi1020/guess-mind.git",
  "author": "ddamjengi <silluat11@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babelify": "^10.0.0",
    "cookie-parser": "^1.4.4",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-bro": "^2.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "pug": "^2.0.4",
    "socket.io": "^2.4.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/server --ignore src/assets --ignore src/static",
    "dev:assets": "cd src && gulp dev",
    "build:server": "babel src --out-dir build --ignore src/assets,src/static,src/gulpfile.babel.js",
    "build:assets": "cd src && gulp build",
    "copyAll": "cp -R src/views build && cp -R src/static build",
    "build:del": "rm -rf build",
    "build": "npm run build:del && npm run build:server && npm run build:assets && npm run copyAll",
    "start": "node build/server.js"
  }
}
